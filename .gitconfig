[user]
	name = walker smith
	email = walker.smith@versionone.com

[push]
	default = simple

[alias]
	alias = config --get-regexp 'alias.*'
	co = checkout
	ci = commit
	a = add
	aa = add --all
	s = status
	br = branch
	p = pull
	pr = pull --rebase
	cp = cherry-pick -X
	pick = cherry-pick
	m = merge
	ours = checkout --ours --
	theirs = checkout --theirs --
	mt = mergetool
	type = cat-file -t
	dump = cat-file -p
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	cleanup = !git reset --hard && git clean -f
	undo = reset --soft HEAD^

[core]
	editor = 'C:/Program Files/Sublime Text 3/sublime_text.exe' --wait --new-window

[gui]
	gcwarning = false

[diff]
	tool = bc4
	guitool = bc4

[difftool "bc4"]
	path = c:\\Program Files (x86)\\Beyond Compare 4\\
	cmd = 'c:\\Program Files (x86)\\Beyond Compare 4\\BComp.exe' \"$LOCAL\" \"$REMOTE\"

[difftool]
	prompt = false

[merge]
	tool = bc4

[mergetool]
	keepBackup = false

[mergetool "bc4"]
	path = c:\\Program Files (x86)\\Beyond Compare 4\\
	cmd = 'c:\\Program Files (x86)\\Beyond Compare 4\\BComp.exe' \"$LOCAL\" \"$REMOTE\" \"$BASE\" \"$MERGED\"
	trustExitCode = true